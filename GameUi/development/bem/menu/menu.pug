mixin menu(menuObject)
    .menu
        ul.menu_list
            each val,index in menuObject
                if index == 0
                    li.menu_item.active(class=val.className id=val.id)
                        .menu_img
                            svg.information_icons
                                path( d=val.path)
                        = val.menuName
                else
                    li.menu_item(class=val.className id=val.id)
                        .menu_img
                            svg.information_icons
                                path( d=val.path)
                        = val.menuName
